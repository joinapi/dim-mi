<proxy xmlns="http://ws.apache.org/ns/synapse" name="wsdlEndPoint_Receiving_Sequence_Dynamic_BM" transports="http,https">
        <target>
            <inSequence>
                <property name="receiving_sequence" value="dynamic_receiving_sequence" scope="default" type="STRING"/>
                <send buildmessage="true" receive="{get-property('receiving_sequence')}">
                    <endpoint>
                        <wsdl uri="http://localhost:9000/services/SimpleStockQuoteService?wsdl" service="SimpleStockQuoteService" port="SimpleStockQuoteServiceHttpSoap11Endpoint"/>
                    </endpoint>
                </send>
            </inSequence>
            <outSequence>
                <drop/>
            </outSequence>
        </target>
    </proxy>
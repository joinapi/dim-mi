<proxy xmlns="http://ws.apache.org/ns/synapse" name="propertyFloatOperationTestProxy" transports="http,https">
    <target>
        <inSequence>
            <property xmlns="http://ws.apache.org/ns/synapse" name="FloatVal" value="123.123"  scope="operation" type="FLOAT" />
            <log level="custom">
                <property xmlns:ns="http://org.apache.synapse/xsd" xmlns:ns3="http://org.apache.synapse/xsd" name="symbol" expression="get-property('operation', 'FloatVal')" />
            </log>
            <payloadFactory media-type="xml">
                <format>
                    <m:symbol xmlns:m="http://services.samples">$1</m:symbol>
                </format>
                <args>
                    <arg xmlns:m0="http://services.samples" expression="get-property('operation', 'FloatVal')"/>
                </args>
            </payloadFactory>
            <respond />
        </inSequence>
    </target>
</proxy>
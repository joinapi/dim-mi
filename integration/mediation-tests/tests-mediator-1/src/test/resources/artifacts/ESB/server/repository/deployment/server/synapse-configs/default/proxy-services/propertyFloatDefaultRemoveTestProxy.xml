<proxy xmlns="http://ws.apache.org/ns/synapse" name="propertyFloatDefaultRemoveTestProxy" transports="http,https">
    <target>
        <inSequence>
            <property xmlns="http://ws.apache.org/ns/synapse" name="FloatVal" value="123.123"  scope="default" type="FLOAT" />
            <log level="custom">
                <property xmlns:ns="http://org.apache.synapse/xsd" xmlns:ns3="http://org.apache.synapse/xsd" name="symbol" expression="$ctx:FloatVal" />
            </log>

            <property xmlns="http://ws.apache.org/ns/synapse" name="FloatVal" action="remove"  scope="default" />
            <log level="custom">
                <property xmlns:ns="http://org.apache.synapse/xsd" xmlns:ns3="http://org.apache.synapse/xsd" name="symbol" expression="$ctx:FloatVal" />
            </log>

            <payloadFactory media-type="xml">
                <format>
                    <m:symbol xmlns:m="http://services.samples">Property Set and Removed</m:symbol>
                </format>
            </payloadFactory>
            <respond />
        </inSequence>
    </target>
</proxy>